<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:nested="http://www.griddynamics.com/schema/nested"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.griddynamics.com/schema/nested http://www.griddynamics.com/schema/nested/nested-schema.xsd
       ">

    <!-- Old fashion way using boilerplate xml wasn't changed here
        due to check it's working every time by running unit-tests -->
    <bean name="firstObject" factory-bean="root" factory-method="lookup" lazy-init="true">
        <constructor-arg value="firstObject"/>
        <constructor-arg value="com.griddynamics.spring.nested.analyzer.SubInterface"/>
    </bean>

    <bean name="secondObject" factory-bean="root" factory-method="lookup" lazy-init="true">
        <constructor-arg value="secondObject"/>
        <constructor-arg value="com.griddynamics.spring.nested.analyzer.SubInterface"/>
    </bean>

    <nested:import id="thirdObject" interface="com.griddynamics.spring.nested.analyzer.SubInterface"/>

    <nested:import id="fourthObject" interface="com.griddynamics.spring.nested.analyzer.SubInterface"/>
</beans>