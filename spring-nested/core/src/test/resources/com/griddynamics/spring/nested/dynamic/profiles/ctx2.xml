<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:nested="http://www.griddynamics.com/schema/nested"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.griddynamics.com/schema/nested http://www.griddynamics.com/schema/nested/nested-schema.xsd
       ">

    <nested:import id="object1" interface="com.griddynamics.spring.nested.analyzer.SubInterface"/>

    <bean name="object2" class="com.griddynamics.spring.nested.analyzer.TransportObject" lazy-init="true"/>
    <nested:export ref="object2" interface="com.griddynamics.spring.nested.analyzer.SuperInterface"/>

    <bean name="context2" class="com.griddynamics.spring.nested.analyzer.TransportObject" lazy-init="true"/>
    <nested:export ref="context2" interface="com.griddynamics.spring.nested.analyzer.SuperInterface"/>
</beans>