<beans:beans xmlns="http://www.macys.com/schema/nested"
       xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
         http://www.macys.com/schema/nested http://www.macys.com/schema/nested/nested-schema.xsd
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <beans:import resource="first-context.xml" />

    <export ref="firstChild" interface="com.macys.platform.util.spring.nested.xml.Child" />

    <export ref="parent" interface="com.macys.platform.util.spring.nested.xml.Parent" />

    <import id="secondChild" interface="com.macys.platform.util.spring.nested.xml.Child" />
</beans:beans>